{
  "name": "internationalization",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --i18n-format xlf --output-path i18n --i18n-locale es && xliffmerge --profile xliffmerge.json",
    "start-en": "ng serve --configuration=en",
    "start-es": "ng serve --configuration=es",
    "build": "ng build --base-href /internationalization/",
    "build-en": "ng build --aot --output-path dist/en --base-href /internationalization/en --prod --i18n-file src/i18n/messages.en.xlf --i18n-format xlf --i18n-locale en",
    "build-es": "ng build --aot --output-path dist/es --base-href /internationalization/es --i18n-file src/i18n/messages.es.xlf --i18n-format xlf --i18n-locale es",
    "build-all": "npm run build-es & npm run build-en",
    "build-i18n": "for lang in en es; do ng build --output-path=dist/$lang --aot -prod --bh /$lang/ --i18n-file=src/i18n/messages.$lang.xlf --i18n-format=xlf --locale=$lang; done"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngx-i18nsupport/tooling": "^1.0.1",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "core-js": "^2.5.4",
    "ngx-i18nsupport": "^0.16.3",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.9",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.0.1"
  }
}
